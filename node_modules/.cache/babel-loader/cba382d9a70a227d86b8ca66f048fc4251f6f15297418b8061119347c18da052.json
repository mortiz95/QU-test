{"ast":null,"code":"import { defineComponent, onBeforeMount, ref } from \"vue\";\nexport default defineComponent({\n  name: \"TableAPI\",\n  props: {\n    data: {\n      type: Object,\n      required: true\n    }\n  },\n  setup(props) {\n    const sortedData = ref([]);\n    const sortKey = ref('');\n    const sortDirection = ref('asc');\n    onBeforeMount(() => {\n      sortedData.value = sortData();\n    });\n    const sortBy = key => {\n      if (key === sortKey.value) {\n        sortDirection.value = sortDirection.value === 'asc' ? 'desc' : 'asc';\n      } else {\n        sortKey.value = key;\n        sortDirection.value = 'asc';\n      }\n      sortedData.value = sortData();\n    };\n    const sortData = () => {\n      const sorted = [...props.data];\n      sorted.sort((a, b) => {\n        const modifier = sortDirection.value === 'asc' ? 1 : -1;\n        if (a[sortKey.value] < b[sortKey.value]) return -1 * modifier;\n        if (a[sortKey.value] > b[sortKey.value]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    };\n    return {\n      sortedData,\n      sortKey,\n      sortDirection,\n      sortBy\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","onBeforeMount","ref","name","props","data","type","Object","required","setup","sortedData","sortKey","sortDirection","value","sortData","sortBy","key","sorted","sort","a","b","modifier"],"sources":["/Users/matiasortiz/qu-test-api-table/src/components/Table.vue"],"sourcesContent":["\n<template>\n  <table>\n    <thead>\n      <tr>\n        <th @click=\"sortBy('id')\">ID</th>\n        <th @click=\"sortBy('name')\">Name</th>\n        <th @click=\"sortBy('age')\">Age</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr v-for=\"item in sortedData\" :key=\"item.id\">\n        <td>{{ item.id }}</td>\n        <td>{{ item.name }}</td>\n        <td>{{ item.age }}</td>\n      </tr>\n    </tbody>\n  </table>\n</template>\n\n<script>\nimport { defineComponent, onBeforeMount, ref } from \"vue\";\nexport default defineComponent({\n  name: \"TableAPI\",\n  props: {\n    data: {\n      type: Object,\n      required: true,\n    },\n  },\n\n  setup(props){\n\n    const sortedData = ref([])\n    const sortKey = ref('')\n    const sortDirection = ref('asc')\n\n    onBeforeMount( () => {\n      sortedData.value = sortData();\n    });\n\n    const sortBy = (key) => {\n      if (key === sortKey.value) {\n        sortDirection.value = sortDirection.value === 'asc' ? 'desc' : 'asc';\n      } else {\n        sortKey.value = key;\n        sortDirection.value = 'asc';\n      }\n\n      sortedData.value = sortData();\n    \n    }\n\n     const sortData = () => {\n      const sorted = [...props.data];\n      sorted.sort((a, b) => {\n        const modifier = sortDirection.value === 'asc' ? 1 : -1;\n        if (a[sortKey.value] < b[sortKey.value]) return -1 * modifier;\n        if (a[sortKey.value] > b[sortKey.value]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    }\n\n    return {\n      sortedData,\n      sortKey,\n      sortDirection,\n      sortBy\n    };\n  }\n  \n});\n</script>\n"],"mappings":"AAqBA,SAASA,eAAe,EAAEC,aAAa,EAAEC,GAAE,QAAS,KAAK;AACzD,eAAeF,eAAe,CAAC;EAC7BG,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EAEDC,KAAKA,CAACL,KAAK,EAAC;IAEV,MAAMM,UAAS,GAAIR,GAAG,CAAC,EAAE;IACzB,MAAMS,OAAM,GAAIT,GAAG,CAAC,EAAE;IACtB,MAAMU,aAAY,GAAIV,GAAG,CAAC,KAAK;IAE/BD,aAAa,CAAE,MAAM;MACnBS,UAAU,CAACG,KAAI,GAAIC,QAAQ,CAAC,CAAC;IAC/B,CAAC,CAAC;IAEF,MAAMC,MAAK,GAAKC,GAAG,IAAK;MACtB,IAAIA,GAAE,KAAML,OAAO,CAACE,KAAK,EAAE;QACzBD,aAAa,CAACC,KAAI,GAAID,aAAa,CAACC,KAAI,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;MACtE,OAAO;QACLF,OAAO,CAACE,KAAI,GAAIG,GAAG;QACnBJ,aAAa,CAACC,KAAI,GAAI,KAAK;MAC7B;MAEAH,UAAU,CAACG,KAAI,GAAIC,QAAQ,CAAC,CAAC;IAE/B;IAEC,MAAMA,QAAO,GAAIA,CAAA,KAAM;MACtB,MAAMG,MAAK,GAAI,CAAC,GAAGb,KAAK,CAACC,IAAI,CAAC;MAC9BY,MAAM,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACpB,MAAMC,QAAO,GAAIT,aA