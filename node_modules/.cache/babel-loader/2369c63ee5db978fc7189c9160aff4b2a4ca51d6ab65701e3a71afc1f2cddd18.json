 },\n  },\n\n  methods: {\n    sortBy(key) {\n      if (key === this.sortKey) {\n        this.sortDirection = this.sortDirection === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.sortKey = key;\n        this.sortDirection = 'asc';\n      }\n\n      this.sortedData = this.sortData();\n    },\n    sortData() {\n      const sorted = [...this.data];\n      sorted.sort((a, b) => {\n        const modifier = this.sortDirection === 'asc' ? 1 : -1;\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    },\n  },\n  created() {\n    this.sortedData = this.sortData();\n  },\n  \n});\n</script>\n"],"mappings":"AAqBA,SAASA,eAAc,QAAS,KAAK;AACrC,eAAeA,eAAe,CAAC;EAC7BC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEC,OAAO;IAAEC;EAAU,CAAC;EAClCC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EAEDC,OAAO,EAAE;IACPC,MAAMA,CAACC,GAAG,EAAE;MACV,IAAIA,GAAE,KAAM,IAAI,CAACC,OAAO,EAAE;QACxB,IAAI,CAACC,aAAY,GAAI,IAAI,CAACA,aAAY,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;MACpE,OAAO;QACL,IAAI,CAACD,OAAM,GAAID,GAAG;QAClB,IAAI,CAACE,aAAY,GAAI,KAAK;MAC5B;MAEA,IAAI,CAACC,UAAS,GAAI,IAAI,CAACC,QAAQ,CAAC,CAAC;IACnC,CAAC;IACDA,QAAQA,CAAA,EAAG;MACT,MAAMC,MAAK,GAAI,CAAC,GAAG,IAAI,CAACX,IAAI,CAAC;MAC7BW,MAAM,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACpB,MAAMC,QAAO,GAAI,IAAI,CAACP,aAAY,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QACtD,IAAIK,CAAC,CAAC,IAAI,CAACN,OAAO,IAAIO,CAAC,CAAC,IAAI,CAACP,OAAO,CAAC,EAAE,OAAO,CAAC,IAAIQ,QAAQ;QAC3D,IAAIF,CAAC,CAAC,IAAI,CAACN,OAAO,IAAIO,CAAC,CAAC,IAAI,CAACP,OAAO,CAAC,EAAE,OAAO,IAAIQ,QAAQ;QAC1D,OAAO,CAAC;MACV,CAAC,CAAC;MACF,OAAOJ,MAAM;IACf;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,UAAS,GAAI,IAAI,CAACC,QAAQ,CAAC,CAAC;EACnC;AAEF,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}