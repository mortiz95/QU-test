{"ast":null,"code":"import { defineComponent, onBeforeMount } from \"vue\";\nexport default defineComponent({\n  name: \"TableAPI\",\n  props: {\n    data: {\n      type: Object,\n      required: true\n    }\n  },\n  setup(props) {\n    const sortedData = ref([]);\n    const sortKey = ref('');\n    const sortDirection = ref('asc');\n    onBeforeMount(() => {\n      sortedData.value = sortData();\n    });\n    const sortBy = key => {\n      if (key === sortKey.value) {\n        sortDirection = sortDirection === 'asc' ? 'desc' : 'asc';\n      } else {\n        sortKey = key;\n        sortDirection = 'asc';\n      }\n      this.sortedData = this.sortData();\n    };\n    const sortData = () => {\n      const sorted = [...props.data];\n      sorted.sort((a, b) => {\n        const modifier = sortDirection.value === 'asc' ? 1 : -1;\n        if (a[sortKey.value] < b[sortKey.value]) return -1 * modifier;\n        if (a[sortKey.value] > b[sortKey.value]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    };\n    return {\n      sortedData,\n      sortKey,\n      sortDirection\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","onBeforeMount","name","props","data","type","Object","required","setup","sortedData","ref","sortKey","sortDirection","value","sortData","sortBy","key","sorted","sort","a","b","modifier"],"sources":["/Users/matiasortiz/qu-test-api-table/src/components/Table.vue"],"sourcesContent":["\n<template>\n  <table>\n    <thead>\n      <tr>\n        <th @click=\"sortBy('id')\">ID</th>\n        <th @click=\"sortBy('name')\">Name</th>\n        <th @click=\"sortBy('age')\">Age</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr v-for=\"item in sortedData\" :key=\"item.id\">\n        <td>{{ item.id }}</td>\n        <td>{{ item.name }}</td>\n        <td>{{ item.age }}</td>\n      </tr>\n    </tbody>\n  </table>\n</template>\n\n<script>\nimport { defineComponent, onBeforeMount } from \"vue\";\nexport default defineComponent({\n  name: \"TableAPI\",\n  props: {\n    data: {\n      type: Object,\n      required: true,\n    },\n  },\n\n  setup(props){\n\n    const sortedData = ref([])\n    const sortKey = ref('')\n    const sortDirection = ref('asc')\n\n    onBeforeMount( () => {\n      sortedData.value = sortData();\n    });\n\n    const sortBy = (key) => {\n      if (key === sortKey.value) {\n        sortDirection = sortDirection === 'asc' ? 'desc' : 'asc';\n      } else {\n        sortKey = key;\n        sortDirection = 'asc';\n      }\n\n      this.sortedData = this.sortData();\n    \n    }\n\n     const sortData = () => {\n      const sorted = [...props.data];\n      sorted.sort((a, b) => {\n        const modifier = sortDirection.value === 'asc' ? 1 : -1;\n        if (a[sortKey.value] < b[sortKey.value]) return -1 * modifier;\n        if (a[sortKey.value] > b[sortKey.value]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    }\n\n    return {\n      sortedData,\n      sortKey,\n      sortDirection,\n    };\n  }\n  \n});\n</script>\n"],"mappings":"AAqBA,SAASA,eAAe,EAAEC,aAAY,QAAS,KAAK;AACpD,eAAeD,eAAe,CAAC;EAC7BE,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EAEDC,KAAKA,CAACL,KAAK,EAAC;IAEV,MAAMM,UAAS,GAAIC,GAAG,CAAC,EAAE;IACzB,MAAMC,OAAM,GAAID,GAAG,CAAC,EAAE;IACtB,MAAME,aAAY,GAAIF,GAAG,CAAC,KAAK;IAE/BT,aAAa,CAAE,MAAM;MACnBQ,UAAU,CAACI,KAAI,GAAIC,QAAQ,CAAC,CAAC;IAC/B,CAAC,CAAC;IAEF,MAAMC,MAAK,GAAKC,GAAG,IAAK;MACtB,IAAIA,GAAE,KAAML,OAAO,CAACE,KAAK,EAAE;QACzBD,aAAY,GAAIA,aAAY,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;MAC1D,OAAO;QACLD,OAAM,GAAIK,GAAG;QACbJ,aAAY,GAAI,KAAK;MACvB;MAEA,IAAI,CAACH,UAAS,GAAI,IAAI,CAACK,QAAQ,CAAC,CAAC;IAEnC;IAEC,MAAMA,QAAO,GAAIA,CAAA,KAAM;MACtB,MAAMG,MAAK,GAAI,CAAC,GAAGd,KAAK,CAACC,IAAI,CAAC;MAC9Ba,MAAM,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACpB,MAAMC,QAAO,GAAIT,aAAa,CAACC,KAAI,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QACvD,IAAIM,CAAC,CAACR,OAAO,CAACE,KAAK,IAAIO,CAAC,CAACT,OAAO,CAACE,KAAK,CAAC,EAAE,