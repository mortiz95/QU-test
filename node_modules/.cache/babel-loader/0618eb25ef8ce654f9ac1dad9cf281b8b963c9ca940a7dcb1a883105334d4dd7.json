.sortData();\n    },\n    sortData() {\n      const sorted = [...this.data];\n      sorted.sort((a, b) => {\n        const modifier = this.sortDirection === 'asc' ? 1 : -1;\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    },\n  },\n  created() {\n    this.sortedData = this.sortData();\n  },\n};\n</script>\n"],"mappings":"AAoBA,eAAe;EACbA,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAACC,GAAG,EAAE;MACV,IAAIA,GAAE,KAAM,IAAI,CAACJ,OAAO,EAAE;QACxB,IAAI,CAACC,aAAY,GAAI,IAAI,CAACA,aAAY,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;MACpE,OAAO;QACL,IAAI,CAACD,OAAM,GAAII,GAAG;QAClB,IAAI,CAACH,aAAY,GAAI,KAAK;MAC5B;MAEA,IAAI,CAACF,UAAS,GAAI,IAAI,CAACM,QAAQ,CAAC,CAAC;IACnC,CAAC;IACDA,QAAQA,CAAA,EAAG;MACT,MAAMC,MAAK,GAAI,CAAC,GAAG,IAAI,CAACR,IAAI,CAAC;MAC7BQ,MAAM,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACpB,MAAMC,QAAO,GAAI,IAAI,CAACT,aAAY,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QACtD,IAAIO,CAAC,CAAC,IAAI,CAACR,OAAO,IAAIS,CAAC,CAAC,IAAI,CAACT,OAAO,CAAC,EAAE,OAAO,CAAC,IAAIU,QAAQ;QAC3D,IAAIF,CAAC,CAAC,IAAI,CAACR,OAAO,IAAIS,CAAC,CAAC,IAAI,CAACT,OAAO,CAAC,EAAE,OAAO,IAAIU,QAAQ;QAC1D,OAAO,CAAC;MACV,CAAC,CAAC;MACF,OAAOJ,MAAM;IACf;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACZ,UAAS,GAAI,IAAI,CAACM,QAAQ,CAAC,CAAC;EACnC;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}