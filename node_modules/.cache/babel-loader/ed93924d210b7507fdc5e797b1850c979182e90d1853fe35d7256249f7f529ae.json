{"ast":null,"code":"import { Console } from \"console\";\nimport { defineComponent } from \"vue\";\nexport default defineComponent({\n  name: \"TableAPI\",\n  props: ['data'],\n  data() {\n    return {\n      sortedData: [],\n      sortKey: '',\n      sortDirection: 'asc'\n    };\n  },\n  methods: {\n    sortBy(key) {\n      if (key === this.sortKey) {\n        this.sortDirection = this.sortDirection === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.sortKey = key;\n        this.sortDirection = 'asc';\n      }\n      this.sortedData = this.sortData();\n    },\n    sortData() {\n      const sorted = [...this.data];\n      sorted.sort((a, b) => {\n        const modifier = this.sortDirection === 'asc' ? 1 : -1;\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    }\n  },\n  created() {\n    console.log(this.sortedData);\n    this.sortedData = this.sortData();\n  }\n});","map":{"version":3,"names":["Console","defineComponent","name","props","data","sortedData","sortKey","sortDirection","methods","sortBy","key","sortData","sorted","sort","a","b","modifier","created","console","log"],"sources":["/Users/matiasortiz/qu-test-api-table/src/components/Table.vue"],"sourcesContent":["<!-- src/components/Table.vue -->\n<template>\n  <table>\n    <thead>\n      <tr>\n        <th @click=\"sortBy('id')\">ID</th>\n        <th @click=\"sortBy('name')\">Name</th>\n        <th @click=\"sortBy('age')\">Age</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr v-for=\"item in sortedData\" :key=\"item.id\">\n        <td>{{ item.id }}</td>\n        <td>{{ item.name }}</td>\n        <td>{{ item.age }}</td>\n      </tr>\n    </tbody>\n  </table>\n</template>\n\n<script>\nimport { Console } from \"console\";\nimport { defineComponent } from \"vue\";\nexport default defineComponent({\n  name: \"TableAPI\",\n  props: ['data'],\n  data() {\n    return {\n      sortedData: [],\n      sortKey: '',\n      sortDirection: 'asc',\n    };\n  },\n  methods: {\n    sortBy(key) {\n      if (key === this.sortKey) {\n        this.sortDirection = this.sortDirection === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.sortKey = key;\n        this.sortDirection = 'asc';\n      }\n\n      this.sortedData = this.sortData();\n    },\n    sortData() {\n      const sorted = [...this.data];\n      sorted.sort((a, b) => {\n        const modifier = this.sortDirection === 'asc' ? 1 : -1;\n        if (a[this.sortKey] < b[this.sortKey]) return -1 * modifier;\n        if (a[this.sortKey] > b[this.sortKey]) return 1 * modifier;\n        return 0;\n      });\n      return sorted;\n    },\n  },\n  created() {\n    console.log(this.sortedData)\n    this.sortedData = this.sortData();\n  },\n});\n</script>\n"],"mappings":"AAqBA,SAASA,OAAM,QAAS,SAAS;AACjC,SAASC,eAAc,QAAS,KAAK;AACrC,eAAeA,eAAe,CAAC;EAC7BC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAACC,GAAG,EAAE;MACV,IAAIA,GAAE,KAAM,IAAI,CAACJ,OAAO,EAAE;QACxB,IAAI,CAACC,aAAY,GAAI,IAAI,CAACA,aAAY,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;MACpE,OAAO;QACL,IAAI,CAACD,OAAM,GAAII,GAAG;QAClB,IAAI,CAACH,aAAY,GAAI,KAAK;MAC5B;MAEA,IAAI,CAACF,UAAS,GAAI,IAAI,CAACM,QAAQ,CAAC,CAAC;IACnC,CAAC;IACDA,QAAQA,CAAA,EAAG;MACT,MAAMC,MAAK,GAAI,CAAC,GAAG,IAAI,CAACR,IAAI,CAAC;MAC7BQ,MAAM,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACpB,MAAMC,QAAO,GAAI,IAAI,CAACT,aAAY,KAAM,KAAI,GAAI,IAAI,CAAC,CAAC;QACtD,IAAIO,CAAC,CAAC,IAAI,CAACR,OAAO,IAAIS,CAAC,CAAC,IAAI,CAACT,OAAO,CAAC,EAAE,OAAO,CAAC,IAAIU,QAAQ;QAC3D,IAAIF,CAAC,CAAC,IAAI,CAACR,OAAO,IAAIS,CAAC,CAAC,IAAI,CAACT,OAAO,CAAC,EAAE,OAAO,IAAIU,QAAQ;QAC1D,OAAO,CAAC;MACV,CAAC,CAAC;MACF,OAAOJ,MAAM;IACf;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,UAAU;IAC3B,IAAI,CAACA,UAAS,GAAI,IAAI,CAACM,QAAQ,CAAC,CAAC;EACnC;AACF,CAAC,C